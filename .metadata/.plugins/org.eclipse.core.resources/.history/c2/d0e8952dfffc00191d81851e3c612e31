			<% for(int black = 0; black < 5; black++){ %>
	 			<% if(black%2 == 0){ %>
	 			<td><div class="black"></div></td>
	 			<% } %>
	 			<% if(black%2 != 0){ %>
	 			<td><div class="red"></div></td>
	 			<% } %>
			<%} %>
	 		</tr>
	 		<% } %>
	 	
