<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Demo JSP</title>
</head>
<body>
<h1>Hello World</h1>
	<%!
	public int add(int a, int b){
		return a+b;
	}
	%>
	<%
	int i = 2;
	int j = 3;
	%>
	<h3><%= add(i,j) %></h3>
	<% for(int index = 0; index < 5; index++){ %>
	 <h2><%= i %></h2>
	<%} %>
	
	
</body>
